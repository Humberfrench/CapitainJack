<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Jack.Model" namespace="Jack.Model">
  <class name="Reuniao" table="tb_reuniao_agendada">
		<id name="Codigo" column="id_reuniao">
			<generator class="native"/>
		</id>
    <property column="nr_ano" name="Ano" not-null="true"/>
