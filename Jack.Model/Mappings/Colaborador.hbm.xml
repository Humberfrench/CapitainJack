<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="CECAMAdm.Struct" namespace="CECAMAdm.Struct">
	<class name="Colaborador" table="tb_colaborador">
		<id name="Codigo" column="id_colaborador">
			<generator class="native"/>
		</id>
    <property column="ds_nome" name="Nome" not-null="true"/>
    <property column="ds_fone" name="Telefone" not-null="false"/>
    <property column="ds_celular" name="Celular" not-null="false"/>
    <property column="ds_email" name="EMail" not-null="false"/>
    <property column="nr_ano_notificacao" name="AnoNotificacao" not-null="false"/>
    <property column="nr_cpf" name="CPF" not-null="false"/>
  </class>
</hibernate-mapping>

