<configuration>
  <configSections>
    <section name="hibernate-configuration" type="NHibernate.Cfg.ConfigurationSectionHandler, NHibernate" />
    <sectionGroup name="elmah">
      <section name="security" requirePermission="false" type="Elmah.SecuritySectionHandler, Elmah" />
      <section name="errorLog" requirePermission="false" type="Elmah.ErrorLogSectionHandler, Elmah" />
      <section name="errorMail" requirePermission="false" type="Elmah.ErrorMailSectionHandler, Elmah" />
      <section name="errorFilter" requirePermission="false" type="Elmah.ErrorFilterSectionHandler, Elmah" />
    </sectionGroup>
  </configSections>
  <hibernate-configuration xmlns="urn:nhibernate-configuration-2.2">
    <session-factory>
      <property name="dialect">NHibernate.Dialect.MsSql2008Dialect</property>
      <property name="connection.provider">NHibernate.Connection.DriverConnectionProvider</property>
      <property name="connection.connection_string">Server=.\web; Initial Catalog=dbJ; User ID=sa; Password=123456</property>
      <!--<property name="connection.connection_string">Server=.\WEB; Initial Catalog=dbCECAM; User ID=sa; Password=123456</property>-->
      <!--<property name="proxyfactory.factory_class">NHibernate.Bytecode.DefaultProxyFactoryFactory, NHibernate</property>-->
      <mapping assembly="Jack.Domain" />
    </session-factory>
  </hibernate-configuration>
  <connectionStrings>
    <add name="JackKey" connectionString="Data Source=.\web;Initial Catalog=dbJ;Integrated Security=False;User ID=sa;Password=123456" providerName="System.Data.SqlClient" />
    <add name="JackKeyHome" connectionString="Data Source=.\Web;Initial Catalog=dbJ;Integrated Security=False;User ID=sa;Password=123456" providerName="System.Data.SqlClient" />
    <add name="elmah-sqlserver" connectionString="Data Source=.\web;User ID=sa;Password=123456;Initial Catalog=dbJ;" providerName="System.Data.SqlClient" />
    <add name="elmah-sqlserver-home" connectionString="Data Source=.\Web;User ID=sa;Password=123456;Initial Catalog=dbJ;" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <!-- Configurações da aplicação -->
    <add key="NomeLogo" value="Jack" />
    <add key="NomeTitulo" value="Gerenciamento de Sacolas de Natal CECAM" />
    <add key="NomeRodape" value="French Solution" />
    <add key="Dominio" value="French" />
    <!-- Configurações da aplicação -->
  </appSettings>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.5" />
      </system.Web>
  -->
  <system.web>
    <httpCookies httpOnlyCookies="true" requireSSL="false" lockItem="true" />
    <globalization culture="pt-BR" uiCulture="pt-BR" />
    <customErrors mode="Off">
      <error statusCode="404" redirect="~/404" />
    </customErrors>
    <authentication mode="None"></authentication>
    <compilation targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
    <httpModules>
      <add name="ErrorLog" type="Elmah.ErrorLogModule, Elmah" />
      <add name="ErrorMail" type="Elmah.ErrorMailModule, Elmah" />
      <add name="ErrorFilter" type="Elmah.ErrorFilterModule, Elmah" />
    </httpModules>
  </system.web>
  <system.webServer>
    <staticContent>
      <remove fileExtension=".woff2" />
      <mimeMap fileExtension=".woff2" mimeType="application/font-woff2" />
    </staticContent>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
    <httpProtocol>
      <customHeaders>
        <add name="X-Frame-Options" value="DENY" />
        <add name="X-UA-Compatible" value="IE=edge,chrome=1" />
      </customHeaders>
    </httpProtocol>
    <validation validateIntegratedModeConfiguration="false" />
    <modules>
      <add name="ErrorLog" type="Elmah.ErrorLogModule, Elmah" preCondition="managedHandler" />
      <add name="ErrorMail" type="Elmah.ErrorMailModule, Elmah" preCondition="managedHandler" />
      <add name="ErrorFilter" type="Elmah.ErrorFilterModule, Elmah" preCondition="managedHandler" />
    </modules>
  </system.webServer>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
